{
  "name": "dawndelvers",
  "version": "1.0.0",
  "description": "eCommerce Boosting/Coaching service for gamers built with MERN Stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"tsup backend/emails/WelcomeEmail.jsx --format esm,cjs --watch\" \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d",
    "email": "email dev --dir backend/emails --port 3001"
  },
  "author": "Miroslav Mihok",
  "license": "MIT",
  "dependencies": {
    "@react-email/components": "^0.0.16",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.0",
    "react-email": "^2.1.1",
    "resend": "^3.2.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.4",
    "nodemon": "^3.0.3",
    "tsup": "^8.0.2"
  }
}
